@model Kish_AndreiCezar_Project.Models.CarModel
@{
    ViewData["Title"] = "Sterge model";
}

<h2>Stergi @Model.Name?</h2>
<p>Producator: @Model.Manufacturer?.Name</p>

<form asp-action="Delete">
    <input type="hidden" asp-for="Id" />
    <button type="submit" class="btn btn-danger">Confirma</button>
    <a asp-action="Index" class="btn btn-secondary">Renunta</a>
</form>

